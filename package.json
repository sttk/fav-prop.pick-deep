{
  "name": "@fav/prop.pick-deep",
  "version": "0.0.0",
  "description": "Creates a plain object and copys child and descendant properties of a source object only specified deeply",
  "main": "index.js",
  "files": [
    "index.js",
    "web/"
  ],
  "scripts": {
    "lint": "eslint .",
    "test": "mocha",
    "coverage": "nyc --reporter=lcov --reporter=text-summary npm test",
    "coveralls": "nyc --reporter=text-lcov npm test | coveralls",
    "web:build": "npm run web:build-src && npm run web:build-test",
    "web:build-src": "browserify index.js --standalone fav.prop.pick-deep -o web/fav.prop.pick-deep.js && cd web && uglifyjs fav.prop.pick-deep.js --compress --mangle -o fav.prop.pick-deep.min.js --source-map url=fav.prop.pick-deep.min.js.map",
    "web:build-test": "node tool/mktest.js",
    "chrome:install": "npm i --no-save mocha-chrome",
    "chrome:test": "mocha-chrome test/web/browser-test.html",
    "phantom:install": "npm i --no-save phantomjs-prebuilt mocha-phantomjs",
    "phantom:test": "mocha-phantomjs -p node_modules/.bin/phantomjs test/web/browser-test.html",
    "build": "npm run lint && npm run coverage && npm run web:build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sttk/fav-prop.pick-deep.git"
  },
  "keywords": [
    "pick",
    "property",
    "tree",
    "enumerable",
    "object",
    "prop",
    "fav"
  ],
  "author": "Takayuki Sato",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sttk/fav-prop.pick-deep/issues"
  },
  "homepage": "https://github.com/sttk/fav-prop.pick-deep#readme",
  "dependencies": {
    "@fav/prop.assign-deep": "^1.0.0",
    "@fav/prop.set-deep": "^1.0.0",
    "@fav/type.is-array": "^1.0.0",
    "@fav/type.is-plain-object": "^1.0.0"
  },
  "devDependencies": {
    "@fav/prop.visit": "^1.0.0",
    "browserify": "^14.5.0",
    "chai": "^3.5.0",
    "coveralls": "^3.0.0",
    "eslint": "^4.11.0",
    "mocha": "^3.4.2",
    "nyc": "^11.3.0",
    "uglify-js": "^3.1.9"
  }
}
